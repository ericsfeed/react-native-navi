{
  "name": "navi-demo",
  "version": "0.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "homepage": "http://ericsfeed.github.io/react-native-navi",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint . 'src/**/*.tsx' './*tsx' --ext .tsx",
